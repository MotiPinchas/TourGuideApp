<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tourguideapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/h_dizingof_square"
        android:label="@string/app_name"
        android:roundIcon="@drawable/h_dizingof_square"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SiteCategoryClass"
            android:label="@string/site_category"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".SiteActivity"
            android:label="Site description"
            android:parentActivityName=".SiteCategoryClass"></activity>
    </application>

</manifest>